//return the amount of years and months that have passed
//set variables -- current date 
 


var today = new Date

var pastMonth = 12;
var pastYear = 2014;

var newDate = new Date("2014-06-17")
  // // math problem to find differnce of current date and previous date 
var diffDays = parseInt((today - newDate) / (1000 * 60 * 60 * 24), 10); 

    // // return differnce in years and months 
var diffDays = diffDays/365;

var n = diffDays.toFixed(2);
console.log(n);
